<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane fx:id="myBorderPane" minHeight="-Infinity" minWidth="-Infinity" prefWidth="1200.0" styleClass="custom-border-pane" stylesheets="@styleSheetUI.css" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.mediaplayereasv.HelloController">
    <bottom>
        <VBox BorderPane.alignment="CENTER">
            <children>
            <Label fx:id="songDisplay" alignment="CENTER" prefWidth="1000000.0" text="Current Song">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
                  <Label fx:id="myDuration" alignment="CENTER" prefWidth="100000.0" text="Duration: 00/00" />
                <HBox alignment="CENTER" prefHeight="45.0" prefWidth="850.0">
                    <children>
                        <Slider fx:id="mySliderDuration" blockIncrement="1.0" onMouseReleased="#onSliderDragged" HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets bottom="15.0" />
                            </HBox.margin></Slider>
                    </children>
                    <padding>
                        <Insets left="15.0" right="15.0" />
                    </padding>
                </HBox>
            <GridPane>
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0" GridPane.columnIndex="1">
                          <children>
                              <Button fx:id="bntShuffel" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0">
                                  <graphic>
                                      <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                                          <image>
                                              <Image url="@../../../bntImages/bntShuffel.png" />
                                          </image>
                                      </ImageView>
                                  </graphic>
                              </Button>
                              <Button fx:id="bntStop" mnemonicParsing="false" onAction="#onStopPlay" prefHeight="40.0" prefWidth="40.0">
                                  <graphic>
                                      <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                                          <image>
                                              <Image url="@../../../bntImages/bntStop.png" />
                                          </image>
                                      </ImageView>
                                  </graphic>
                              </Button>
                              <Button fx:id="bntBack" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0">
                                  <graphic>
                                      <ImageView fitHeight="25.0" fitWidth="25.0" nodeOrientation="RIGHT_TO_LEFT" pickOnBounds="true" preserveRatio="true">
                                          <image>
                                              <Image url="@../../../bntImages/bntnext.png" />
                                          </image>
                                      </ImageView>
                                  </graphic>
                              </Button>
                              <Button fx:id="bntPlay" cache="true" mnemonicParsing="false" onAction="#onPausePlay" prefHeight="50.0" prefWidth="60.0" textAlignment="CENTER">
                                  <graphic>
                                      <ImageView cache="true" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" smooth="false">
                                          <image>
                                              <Image url="@../../../bntImages/bntplay.png" />
                                          </image>
                                      </ImageView>
                                  </graphic>
                              </Button>
                              <Button fx:id="bntNext" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0">
                                  <graphic>
                                      <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                                          <image>
                                              <Image url="@../../../bntImages/bntnext.png" />
                                          </image>
                                      </ImageView>
                                  </graphic>
                              </Button>
                              <Button fx:id="bntRepeat" mnemonicParsing="false" onAction="#onRepeatEnable" prefHeight="40.0" prefWidth="40.0">
                                  <graphic>
                                      <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                                          <image>
                                              <Image url="@../../../bntImages/bntRepeat.png" />
                                          </image>
                                      </ImageView>
                                  </graphic>
                              </Button>
                              <Button fx:id="bntMute" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#onMuteToggle" prefHeight="40.0" prefWidth="40.0">
                                  <graphic>
                                      <ImageView fx:id="bntMuteIcon" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                                          <image>
                                              <Image url="@../../../bntImages/sVolume.png" />
                                          </image>
                                      </ImageView>
                                  </graphic>
                              </Button>
                          </children>
                          <GridPane.margin>
                              <Insets bottom="15.0" top="15.0" />
                          </GridPane.margin>
                      </HBox>
                   <Slider fx:id="mySliderVolume" max="100" min="0" onMouseReleased="#onVolumeChange" showTickLabels="true" value="50" GridPane.columnIndex="2">
                     <GridPane.margin>
                        <Insets top="20.0" />
                     </GridPane.margin></Slider>
               </children>
               <padding>
                  <Insets bottom="15.0" left="15.0" right="15.0" />
               </padding>
            </GridPane>
            </children>
        </VBox>
    </bottom>
   <right>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <ListView fx:id="lvCurrentPlayList" onMouseClicked="#onSongSelected" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </VBox.margin></ListView>
         </children>
      </VBox>
   </right>
   <left>
      <VBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
            <ListView fx:id="lvAllPlayLists" editable="true" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </VBox.margin>
            </ListView>
            <HBox alignment="CENTER">
               <children>
                  <Button fx:id="btnAddPlaylist" alignment="CENTER" maxHeight="40.0" maxWidth="40.0" minHeight="40.0" minWidth="40.0" mnemonicParsing="false" onAction="#onAddPlaylistClicked" prefHeight="40.0" prefWidth="40.0" text="+" textAlignment="CENTER">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnDeletePlaylist" alignment="CENTER" maxHeight="40.0" maxWidth="40.0" minHeight="40.0" minWidth="40.0" mnemonicParsing="false" onAction="#onDeletePlaylistClicked" prefHeight="40.0" prefWidth="40.0" text="-">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font></Button>
                  <TextField fx:id="tfPlaylistName" promptText="Enter Playlist Name" visible="false" />
                  <Button fx:id="btnConfirmPlaylist" alignment="CENTER" layoutX="33.0" layoutY="32.0" maxHeight="40.0" maxWidth="40.0" minHeight="40.0" minWidth="40.0" mnemonicParsing="false" onAction="#onConfirmPlaylist" prefHeight="40.0" prefWidth="40.0" text="✔" textAlignment="CENTER" visible="false" wrapText="true">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnCancelPlaylist" layoutX="54.0" layoutY="32.0" maxHeight="40.0" maxWidth="40.0" minHeight="40.0" minWidth="40.0" mnemonicParsing="false" onAction="#onCancelPlaylist" prefHeight="40.0" prefWidth="40.0" text="❌" visible="false">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </left>
   <top>
      <GridPane BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" GridPane.halignment="LEFT" GridPane.valignment="CENTER">
               <GridPane.margin>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </GridPane.margin></ImageView>
            <Label fx:id="myLabelTitel" text="Morning dew" GridPane.halignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font></Label>
            <ComboBox fx:id="cbSearchBar" prefWidth="10000.0" GridPane.columnIndex="1" GridPane.halignment="CENTER">
               <GridPane.margin>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </GridPane.margin></ComboBox>
            <Button fx:id="bntSearch" mnemonicParsing="false" text="Search" GridPane.columnIndex="1" GridPane.halignment="RIGHT">
               <GridPane.margin>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </GridPane.margin>
            </Button>
            <Label fx:id="connectionStatus" />
         </children>
         <BorderPane.margin>
            <Insets bottom="15.0" top="15.0" />
         </BorderPane.margin>
      </GridPane>
   </top>
   <center>
      <ImageView fx:id="ivMainImage" fitHeight="400.0" fitWidth="500.0" pickOnBounds="true" />
   </center>
</BorderPane>
